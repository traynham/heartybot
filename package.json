{
  "name": "heartybot",
  "version": "3.0.0",
  "description": "Pokemon Discord Bot",
  "main": "index.js",
  "scripts": {
    "_test": "jest --coverage",
    "test": "jest --verbose --coverage",
    "start_old": "nodemon index.js --ignore '*.json'",
    "start": "nodemon -r dotenv/config index.js --ignore '*.json' dotenv_config_path=config/.env",
    "dev": "nodemon -r dotenv/config index.js --ignore '*.json' dotenv_config_path=config/.env_dev",
    "doc": "nodemon -e js --ignore jsdoc/ --exec jsdoc -c ./jsdoc/jsdoc.json"
  },
  "keywords": [
    "pokemon",
    "discord",
    "bot",
    "raid"
  ],
  "author": "Jesse Traynham",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.5.2",
    "cheerio": "^1.0.0-rc.3",
    "clean-jsdoc-theme": "^2.2.13",
    "date-fns": "^2.16.1",
    "dateformat": "^3.0.3",
    "discord.js": "^12.2.0",
    "docdash": "^1.2.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "geolib": "^3.3.1",
    "glob": "^7.1.6",
    "lowdb": "^1.0.0",
    "module-alias": "^2.2.2",
    "moment": "^2.27.0",
    "node-fetch": "^2.6.1",
    "node-notifier": "^8.0.0",
    "pokedex-promise-v2": "^3.2.0",
    "pokemongo-game-master": "^2.1.0",
    "puppeteer": "^5.2.1",
    "qrcode": "^1.4.4",
    "quickcord": "^6.3.1",
    "sequelize": "^6.3.3",
    "shortid": "^2.2.15",
    "sqlite3": "^5.0.0",
    "trie-search": "^1.2.11",
    "yargs-parser": "^18.1.3"
  },
  "_moduleAliases": {
    "@config": "config",
    "@core": "core",
    "@data": "data",
    "@help": "data/help",
    "@root": ".",
    "@models": "models",
    "@models_lowdb": "models_lowdb"
  },
  "jest": {
    "coverageDirectory": "./jest/coverage",
    "coveragePathIgnorePatterns": [
      "./models",
      "/node_modules/",
      "update_bosses.js"
    ],
    "moduleNameMapper": {
      "@config(.*)": "<rootDir>/config$1",
      "@core(.*)": "<rootDir>/core$1",
      "@data/(.*)": "<rootDir>/data/$1",
      "@models/(.*)": "<rootDir>/models/$1",
      "@models_lowdb/(.*)": "<rootDir>/models_lowdb/$1"
    },
    "testEnvironment": "node"
  },
  "devDependencies": {
    "better-docs": "^2.3.2",
    "eslint": "^7.13.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6"
  }
}
