{
  "name": "heartybot",
  "version": "3.0.0",
  "description": "Pokemon Discord Bot",
  "main": "index.js",
  "scripts": {
    "_test": "jest --coverage",
    "test": "jest --verbose --coverage --config=.jest.config.js",
    "start_old": "nodemon index.js --ignore '*.json'",
    "start": "nodemon -r dotenv/config index.js --ignore '*.json' dotenv_config_path=config/.env",
    "dev": "nodemon -r dotenv/config index.js --ignore '*.json' dotenv_config_path=config/.env_dev",
    "doc": "nodemon -e js --ignore jsdoc/ --exec jsdoc -c ./jsdoc/jsdoc.json"
  },
  "bin": {
    "hearty": "./bin/hearty.js"
  },
  "keywords": [
    "pokemon",
    "discord",
    "bot",
    "raid"
  ],
  "author": "Jesse Traynham",
  "repository": "github:traynham/heartybot",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.5.3",
    "cheerio": "^1.0.0-rc.5",
    "date-fns": "^2.16.1",
    "dateformat": "^4.0.3",
    "discord.js": "^12.5.1",
    "docdash": "^1.2.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "geolib": "^3.3.1",
    "glob": "^7.1.6",
    "lowdb": "^1.0.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "puppeteer": "^5.5.0",
    "qrcode": "^1.4.4",
    "quickcord": "^6.3.1",
    "sequelize": "^6.3.5",
    "shortid": "^2.2.16",
    "sqlite3": "^5.0.0",
    "trie-search": "^1.3.4",
    "yargs-parser": "^20.1.3"
  },
  "_moduleAliases": {
    "@config": "config",
    "@core": "core",
    "@data": "data",
    "@help": "data/help",
    "@root": ".",
    "@models": "models",
    "@models_lowdb": "models_lowdb"
  },
  "devDependencies": {
    "better-docs": "^2.3.2",
    "eslint": "^7.16.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6"
  }
}
